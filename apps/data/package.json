{
    "name": "@leeft/data",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "analyze:lifting": "bun src/analyze/index.ts lifting",
        "analyze:lifting:cycles": "jq '.[] | {name: .name, start_date: .dates[0], end_date: .dates[1], workout_count: (.workouts | length)}' data/out/cycles-lifting.json",
        "analyze:lifting:print": "bun src/analyze/index.ts print",
        "analyze:trainheroic": "bun src/analyze/index.ts trainheroic",
        "analyze:workouts": "bun src/analyze/index.ts workouts",
        "analyze:exercises:fuzzy": "bun src/analyze/index.ts exercises-fuzzy",
        "analyze:exercises:full": "bun src/analyze/index.ts exercises-full",
        "analyze:exercises:semantic": "bun src/analyze/index.ts exercises-semantic",
        "check": "biome check .",
        "check:fix": "biome check --write .",
        "embeddings": "bun src/embeddings/index.ts",
        "embeddings:generate": "bun src/embeddings/index.ts generate",
        "embeddings:generate:code": "bun src/embeddings/anthropic-embeddings.ts generate",
        "embeddings:generate:json": "bun src/embeddings/json-embeddings.ts generate",
        "embeddings:search": "bun src/embeddings/index.ts search",
        "classify:claude:all": "bun src/classify/index.ts claude-all",
        "classify:claude:single": "bun src/classify/index.ts claude-single",
        "classify:claude:test": "bun src/classify/index.ts claude-test",
        "classify:openai:test": "bun src/classify/index.ts openai-test",
        "combine:all": "bun src/combine/index.ts combine-all",
        "combine:lifting": "bun src/combine/index.ts combine-lifting",
        "compile:all": "bun src/compile/index.ts compile-all",
        "compile:cardio": "bun src/compile/index.ts compile-cardio",
        "compile:google": "bun src/google/index.ts",
        "compile:lifting": "bun src/compile/index.ts compile-lifting",
        "compile:test": "bun src/compile/index.ts test",
        "compile:test-abr": "bun src/compile/index.ts test-abr",
        "compile:test-day": "bun src/compile/index.ts test-day",
        "exercise:compare": "jq -s '[.[0].exercises[].id] as $raw | [.[1][].id] as $classified | ($raw | sort) as $raw_sorted | ($classified | sort) as $classified_sorted | \"Raw: \\($raw | length), Classified: \\($classified | length), Missing IDs: \\($raw_sorted - $classified_sorted), Extra IDs: \\($classified_sorted - $raw_sorted)\"' data/out/exercise-metadata.json data/exercise-classified.json",
        "fitbit:auth": "bun src/fitbit/index.ts auth",
        "fitbit:download": "bun src/fitbit/index.ts download",
        "fitbit:download-all": "bun src/fitbit/index.ts download-all",
        "fitbit:process": "bun src/fitbit/index.ts process",
        "format": "biome format --write .",
        "lint": "biome lint .",
        "trainheroic:compare": "bun src/trainheroic/compare.ts",
        "trainheroic:download": "bun src/trainheroic/index.ts download",
        "trainheroic:supersets": " jq '.saved_workout.workoutSets[] | select(.exercise_count > 1) | {date: .date_completed, exercises: [.workoutSetExercises[].exercise_title]}' data/download/trainheroic/workouts/*.json",
        "typecheck": "tsc --noEmit",
        "knip": "knip"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.59.0",
        "@leeft/utils": "workspace:*",
        "@google-cloud/local-auth": "^3.0.1",
        "@leeft/types": "workspace:*",
        "dotenv": "^16.5.0",
        "google-auth-library": "^10.4.1",
        "googleapis": "^150.0.1",
        "lodash": "^4.17.23",
        "open": "^10.2.0",
        "openai": "^5.8.2",
        "papaparse": "^5.5.3",
        "uuid": "^11.1.0",
        "zod": "catalog:"
    },
    "devDependencies": {
        "@biomejs/biome": "catalog:",
        "@types/lodash": "^4.17.17",
        "@types/node": "^24.8.1",
        "@types/papaparse": "^5.3.16",
        "@types/uuid": "^10.0.0",
        "knip": "^5.66.0",
        "typescript": "catalog:"
    }
}
