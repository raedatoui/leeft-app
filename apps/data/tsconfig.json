{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2024",
        "lib": ["ES2024"],
        "outDir": "./dist",
        "rootDir": "./src",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "resolveJsonModule": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
